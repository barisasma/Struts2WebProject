<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
	<field name="name">
		<field-validator type="requiredstring">
			<message key="phoneName.required"></message>
		</field-validator>
	</field>
	<field name="date">
		<field-validator type="requiredstring">
			<message key="date.required"></message>
		</field-validator>
	</field>
	<field name="os">
		<field-validator type="requiredstring">
			<message key="os.required"></message>
		</field-validator>
	</field>
</validators>